<!-- sidebar menu: : style can be found in sidebar.less -->
{% from "@AdminLTE/Macros/menu.html.twig" import menu_item %}
<ul class="sidebar-menu" data-widget="tree">
    {% if menu is not defined %}
        {% set menu = adminlte_menu(app.request) %}
    {% endif %}
    {% for item in menu %}
        {{ menu_item(item) }}
    {% endfor %}
</ul>
